                                                    <label class="control-label">Disability Certificate No.<span class="text-danger">*</span></label>
                                                    <input class="form-control" type="text" id="txtdisabilitycertificateNumber" name="txtdisabilitycertificateNumber" value="@profile.Disability_Code" readonly>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Postal Address <span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" id="txtpobox" name="txtpobox" value="@profile.Address" readonly>
                                            </div>
                                        </div>


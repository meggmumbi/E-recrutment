                                        <input class="form-control" type="text" id="jobtxtgraduationyear" name="jobtxtgraduationyear">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">Start Date <span class="text-danger">*</span></label>
                                        <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="jobacademictxtstartdate" name="jobacademictxtstartdate"></div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">End Date <span class="text-danger">*</span></label>
                                        <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="jobacademictxtenddate" name="jobacademictxtenddate"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <ul class="list-inline pull-right">
                                    <li>
                                        <button class="btn btn-primary pull-right ladda-button btn_job_academicqualifications" id="btn_job_academicqualifications" name="btn_job_academicqualifications" type="submit" data-style="slide-right" data-background-color="darkgreen">
                                            <span class="ladda-label">
                                                Save Academic Qualification
                                            </span>
                                        </button>
                                    </li>
                                </ul>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="table-responsive">
                                        <div class="panel-body">
                                            <table class="table table-striped custom-table datatable" id="tbl_jobapplication_AcademicQualifications">
                                                <thead>
                                                    <tr>
                                                        <th>#</th>
                                                        <th>Entry No</th>
                                                        <th>Education Level</th>
                                                        <th>Institution</th>
                                                        <th>Country</th>
                                                        <th>Attained Score</th>
                                                        <th>Graduation Year</th>
                                                        <th>Edit</th>
                                                        <th>Delete</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (AcademicQualificationModel academic in Model.AcademicsQualification)
                                                    {
                                                        QualificationsCounter++;
                                                        <tr>
                                                            <td>
                                                                @QualificationsCounter
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => academic.EntryNo)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => academic.EducationLevel)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => academic.Institution)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => academic.Country)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => academic.AttainedScore)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => academic.GraduationYear)
                                                            </td>
                                                            <td>
                                                                <a class="btn btn-success" onclick="JobEditAcademicDetails('@academic.EducationLevel','@academic.Institution','@academic.Country','@academic.AttainedScore')"><i class="fa fa-pencil m-r-10"></i>Edit</a>

                                                            </td>
                                                            <td>
                                                                <a class="btn btn-danger" onclick="JobDeleteAcademicQualificationDetails('@Url.Action("DeletenJobAcademicDetails", "Home", new { EntryNumber = @academic.EntryNo })')"><i class="fa fa-trash m-r-10"></i>Delete</a>

                                                            </td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <ul class="list-inline pull-right">
                                <li>
                                    <button class="btn btn-primary pull-right ladda-button  next-step" type="submit" data-style="slide-right" data-background-color="darkgreen">
                                        <span class="ladda-label">
                                            Next
                                        </span>
                                    </button>
                                </li>
                            </ul>
                        </div>
                    </div>

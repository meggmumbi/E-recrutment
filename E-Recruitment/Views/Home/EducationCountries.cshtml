@model IEnumerable<E_Recruitment.Models.CountriesModel>

@{
    Layout = null;
}
<select class="form-control select2" id="txteducationcountries" name="txteducationcountries" data_live_search="true">
    <option value="">--Select Country---</option>
    <optgroup label="All Countries List">
        @foreach (var item in Model)
        {
            <option value="@item.CountryId">@item.CountryName</option>
        }
    </optgroup>
</select>


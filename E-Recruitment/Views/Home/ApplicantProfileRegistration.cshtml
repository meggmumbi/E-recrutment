@using E_Recruitment.Models
@model dynamic
@{
    ViewBag.Title = "ApplicantProfileRegistration";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int QualificationsCounter = 0;
    int ProffessionalQualificationsCounter = 0;
    int proffesionalbody = 0;
    int workexperienceCounter = 0;
    int RefereeCounter = 0;
    int FileCounter = 0;
}
<div class="panel panel-body">
    <div class="panel panel-primary">
        <div class="row">
            <div class="card">
                <div class="card-header text-center" data-background-color="darkgreen">
                    <h5 class="title"><i><strong>Candidate Registration</strong></i></h5>
                </div>
                <center><h3>Welcome to the E-Recruitment Portal(Complete your Profile)</h3></center>
                <div class="wizard">
                    <div class="wizard-inner">
                        <div class="connecting-line"></div>
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active">
                                <a href="#step1" data-toggle="tab" aria-controls="step1" role="tab" title="Step 1">
                                    <span class="round-tab">
                                        Step 1
                                    </span>
                                    <h5></h5>
                                </a>

                            </li>

                            <li role="presentation" class="disabled">
                                <a href="#step2" data-toggle="tab" aria-controls="step2" role="tab" title="Step 2">
                                    <span class="round-tab">
                                        Step 2
                                    </span>
                                </a>

                            </li>
                            <li role="presentation" class="disabled">
                                <a href="#step3" data-toggle="tab" aria-controls="step3" role="tab" title="Step 3">
                                    <span class="round-tab">
                                        Step 3
                                    </span>
                                </a>

                            </li>
                            <li role="presentation" class="disabled">
                                <a href="#step4" data-toggle="tab" aria-controls="step4" role="tab" title="Step 4">
                                    <span class="round-tab">
                                        Step 4
                                    </span>
                                </a>
                            </li>
                            <li role="presentation" class="disabled">
                                <a href="#step5" data-toggle="tab" aria-controls="step5" role="tab" title="Step 5">
                                    <span class="round-tab">
                                        Step 5
                                    </span>
                                </a>
                            </li>
                            <li role="presentation" class="disabled">
                                <a href="#step6" data-toggle="tab" aria-controls="step6" role="tab" title="Step 6">
                                    <span class="round-tab">
                                        Step 6
                                    </span>
                                </a>
                            </li>
                            <li role="presentation" class="disabled">
                                <a href="#step7" data-toggle="tab" aria-controls="step7" role="tab" title="Step 7">
                                    <span class="round-tab">
                                        Step 7
                                    </span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="tab-content">
                    <div class="tab-pane active" role="tabpanel" id="step1">
                        <div class="panel panel-primary">
                            <div class="panel-heading">
                                Personal Details<div class="pull-right"><i class="fa fa-angle-left"></i>Step 1 of 8<i class="fa fa-angle-right"></i></div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="panel-body">
                                @foreach (ApplicantProfileMode profile in Model.CandidateProfile)
                                {
                                    <div class="row">
                                        <div id="feedback" style="display: none" data-dismiss="alert"></div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Title <span class="text-danger">*</span></label>
                                                <select data-live-search="true" class="form-control input-circle select2" id="prefixtype" name="prefixtype">
                                                    
                                                     <option value="0">Select Title</option>
                                                    <option value="1">Prof</option>
                                                    <option value="2">Doctor</option>
                                                    <option value="3">Mr</option>
                                                    <option value="4">Mrs</option>
                                                    <option value="5">Miss</option>
                                                    <option value="6">Ms</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">SurName <span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" id="txtsurname" name="txtsurname" value="@profile.Last_Name" required>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">First Name<span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" id="txtfirstname" name="txtfirstname" value="@profile.First_Name" required="true">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Other Names</label>
                                                <input class="form-control" type="text" id="txtothernames" value="@profile.Middle_Name">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Gender <span class="text-danger">*</span></label>
                                                <select data-live-search="true" class="form-control input-circle select2" id="txtgender" name="txtgender" >                                                   
                                                   
                                                    <option value="0">--Select Marital Status--</option>
                                                    <option value="1">Male</option>
                                                    <option value="2">Female</option>
                                                    <option value="3">Others</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Marital Status <span class="text-danger">*</span></label>
                                                <select data-live-search="true" class="form-control input-circle select2" id="maritalstatus" name="maritalstatus">
                                                    <option value="@profile.Marital_Status" selected>@profile.Marital_Status</option>
                                                    <option value="">--Select Marital Status--</option>
                                                    <option value="1">Single</option>
                                                    <option value="2">Married</option>
                                                    <option value="3">Divorced</option>
                                                    <option value="4">Others</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Date of Birth <span class="text-danger">*</span></label>
                                                <div class="cal-icon"><input class="form-control datetimepicker" style="" type="text" id="txtdob" name="txtdob" value="@profile.DOB" ></div>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">ID No.<span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" id="txtidnumber" name="txtidnumber" value="@profile.IDNumber">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Religion <span class="text-danger">*</span></label>
                                                <select data-live-search="true" class="form-control input-circle select2" id="txtreligion" name="txtreligion">
                                                    <option value="@profile.Religion" selected>@profile.Religion</option>
                                                    <option value="">--Select Religion--</option>
                                                    <option value="Christianity">Christianity</option>
                                                    <option value="Islam">Islam</option>
                                                    <option value="Hinduism">Hinduism</option>
                                                    <option value="Buddhism">Buddhism</option>
                                                    <option value="Folk Religion">Folk Religion</option>
                                                    <option value="Others">Others</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Passport No</label>
                                                <input class="form-control" type="text" id="txtpassport" name="txtpassport" value="@profile.Passport_No">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Email Address <span class="text-danger">*</span></label>
                                                <input class="form-control" type="email" id="emailaddress" name="emailaddress" value="@profile.Email_Address">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Nationality <span class="text-danger">*</span></label>
                                                @Html.Action("Countries", "Home", @profile.Nationality)
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Post Code <span class="text-danger">*</span></label>
                                                @Html.Action("PostCodes", "Home")
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">City\Town<span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" id="txtcity" name="txtcity" value="@profile.City" readonly>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">County of Residence<span class="text-danger">*</span></label>
                                                @Html.Action("ResidenceCounties", "Home")
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">County of Origin(Home County)<span class="text-danger">*</span></label>
                                                @Html.Action("OriginCounty", "Home")
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Ethnicity<span class="text-danger">*</span></label>
                                                @Html.Action("Ethnicities", "Home")
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">N.H.I.F Number</label>
                                                <input class="form-control" type="text" id="txtnhif" name="txtnhif" value="@profile.NHIF">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">N.S.S.F Number</label>
                                                <input class="form-control" type="text" id="txtnssf" name="txtnssf" value="@profile.NSSF">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Mobile Phone Number <span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" id="txtmobilenumber" name="txtmobilenumber" value="@profile.Mobile_Phone_No">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Alternative Phone Number</label>
                                                <input class="form-control" type="text" id="txtalternativeNumber" name="txtalternativeNumber" value="@profile.Phone_No">
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">KRA Pin <span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" id="txtkrapin" name="txtkrapin"  value="@profile.KRA_Pin">
                                            </div>
                                        </div>                                     
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Are you a person Living with Disability <span class="text-danger">*</span></label>
                                                <select data-live-search="true" class="form-control input-circle select2" id="disabled" name="disabled" onchange="showDiv('hidden_div', this)">
                                                    <option value="False">No</option>
                                                    <option value="True">Yes</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div id="hidden_div" style="display:none">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label">Description of Disability<span class="text-danger">*</span></label>
                                                    <input class="form-control" type="text" id="disabilitydescription" name="disabilitydescription" value="@profile.Disability_Description">
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label">Disability Certificate No.<span class="text-danger">*</span></label>
                                                    <input class="form-control" type="text" id="txtdisabilitycertificateNumber" name="txtdisabilitycertificateNumber" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">Postal Address <span class="text-danger">*</span></label>
                                                <input class="form-control" type="text" id="txtpobox" name="txtpobox" value="@profile.Address">
                                            </div>
                                        </div>
                                    </div>
                                }
                                <center>
                                    <button class="btn btn-primary btn_personalinformations" id="btn_personalinformations" name="btn_personalinformations" type="button">Save Personal Information</button>
                                    <button class="btn btn-primary next-step pull-right" type="button" id="nextBtn" name="nextBtn">Continue</button>

                                </center>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane" role="tabpanel" id="step2">
                        <div class="panel panel-primary">
                            <div class="panel-heading">
                                Academic Qualifications(Starting with highest)<div class="pull-right"><i class="fa fa-angle-left"></i>Step 2 of 5<i class="fa fa-angle-right"></i></div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div id="qualificationsfeedback" style="display: none" data-dismiss="alert"></div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Educational Level<span class="text-danger">*</span></label>
                                            <select data-live-search="true" class="form-control input-circle select2" id="txteducationallevel" name="educationallevel">
                                                <option value="0">--Choose Educationl Level--</option>
                                                <option value="1">Post Doctorate</option>
                                                <option value="2">PHD</option>
                                                <option value="3">Masters</option>
                                                <option value="4">Degree</option>
                                                <option value="5">Post Graduate Diploma</option>
                                                <option value="6">Diploma</option>
                                                <option value="7">Certificate</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">University/Instituition/College <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="txtinstitution" name="txtinstitution">
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Country <span class="text-danger">*</span></label>
                                            @Html.Action("EducationCountries", "Home")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Academic Qualification Title <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="txtqualificationtitle" name="txtqualificationtitle" required>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Specialization Area <span class="text-danger">*</span></label>
                                            @Html.Action("AcademicSpecializationAreas", "Home")
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Attained Score <span class="text-danger">*</span></label>
                                            <select data-live-search="true" class="form-control input-circle select2" id="txtattainedscore" name="txtattainedscore">
                                                <option value="0">--Choose Attained Score--</option>
                                                <option value="1">First Class Hons</option>
                                                <option value="2">Second Class Upper</option>
                                                <option value="3">Second Class Lower</option>
                                                <option value="4">Destinction</option>
                                                <option value="5">Pass</option>
                                                <option value="6">Credit</option>
                                                <option value="7">Others</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Graduation Year <span class="text-danger">*</span></label>
                                            <input class="form-control" type="text" id="txtgraduationyear" name="txtgraduationyear" required>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">Start Date <span class="text-danger">*</span></label>
                                            <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="academictxtstartdate" name="academictxtstartdate" required></div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label">End Date <span class="text-danger">*</span></label>
                                            <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="academictxtenddate" name="academictxtenddate" required></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <ul class="list-inline pull-right">
                                        <li>
                                            <button class="btn btn-primary pull-right ladda-button btn_academicqualifications" id="btn_academicqualifications" name="btn_academicqualifications" type="submit" data-style="slide-right" data-background-color="darkgreen">
                                                <span class="ladda-label">
                                                    Save Academic Qualification
                                                </span>
                                            </button>
                                        </li>
                                    </ul>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-responsive">
                                            <div class="panel-body">
                                                <table class="table table-striped custom-table datatable" id="tbl_applicant_qualifications">
                                                    <thead>
                                                        <tr>
                                                            <th>#</th>
                                                            <th>Entry No</th>
                                                            <th>Education Level</th>
                                                            <th>Institution</th>
                                                            <th>Country</th>
                                                            <th>Attained Score</th>
                                                            <th>Graduation Year</th>
                                                            <th>Edit</th>
                                                            <th>Delete</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>

                                                        @foreach (AcademicQualificationModel academic in Model.AcademicsQualification)
                                                        {
                                                            QualificationsCounter++;
                                                            <tr>
                                                                <td>
                                                                   @QualificationsCounter
                                                                </td>
                                                               
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => academic.EducationLevel)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => academic.Institution)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => academic.Country)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => academic.AttainedScore)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => academic.GraduationYear)
                                                                </td>
                                                            </tr>
                                                        }
                                                      </tbody>

                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                   <div class="row"></div>
                                        <button class="btn btn-primary pull-right ladda-button  next-step" type="submit" data-style="slide-right" data-background-color="darkgreen">
                                            <span class="ladda-label">
                                                Next
                                            </span>
                                        </button>
                                        <button class="btn btn-primary next-step pull-left" type="button" id="nextBtn" name="nextBtn">
                                            <span class="ladda-label">
                                                Previous
                                            </span>
                                        </button>
                            </div>
                        </div>
                        <div id="edit_eacademicQualifications" class="modal custom-modal fade" role="dialog">
                            <div class="modal-dialog">
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                <div class="modal-content modal-lg" style="width:96%">
                                    <div class="modal-header">
                                        <center><h4 class="modal-title">Academic Qualifications</h4></center>
                                    </div>
                                    <div class="modal-body">
                                            <input class="form-control academicentryNumber" type="text" id="academicentryNumber" name="academicentryNumber" style="display:none">
                                            <div class="row" >
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Educational Level<span class="text-danger">*</span></label>
                                                        <select data-live-search="true" class="form-control input-circle select2" id="edittxteducationallevel" name="edittxteducationallevel">
                                                            <option value="0">--Choose Educationl Level--</option>
                                                            <option value="1">Post Doctorate</option>
                                                            <option value="2">PHD</option>
                                                            <option value="3">Masters</option>
                                                            <option value="4">Degree</option>
                                                            <option value="5">Post Graduate Diploma</option>
                                                            <option value="6">Diploma</option>
                                                            <option value="7">Certificate</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">University/Instituition//College <span class="text-danger">*</span></label>
                                                        <input class="form-control" type="text" id="edittxtinstitution" name="edittxtinstitution">
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Country <span class="text-danger">*</span></label>
                                                        @Html.Action("EducationCountries", "Home")
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Academic Qualification Title <span class="text-danger">*</span></label>
                                                        <input class="form-control" type="text" id="edittxtqualificationtitle" name="edittxtqualificationtitle">
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Specialization Area <span class="text-danger">*</span></label>
                                                        @Html.Action("EditSpecializationAreas", "Home")
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Attained Score <span class="text-danger">*</span></label>
                                                        <select data-live-search="true" class="form-control input-circle select2" id="edittxtattainedscore" name="edittxtattainedscore">
                                                            <option value="">--Choose Attained Score--</option>
                                                            <option value="1">First Class Hons</option>
                                                            <option value="2">Second Class Upper</option>
                                                            <option value="3">Second Class Lower</option>
                                                            <option value="4">Destinction</option>
                                                            <option value="5">Pass</option>
                                                            <option value="6">Credit</option>
                                                            <option value="7">Others</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Graduation Year <span class="text-danger">*</span></label>
                                                        <input class="form-control" type="text" id="edittxtgraduationyear" name="edittxtgraduationyear">
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Start Date <span class="text-danger">*</span></label>
                                                        <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="editacademictxtstartdate" name="editacademictxtstartdate"></div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label">End Date <span class="text-danger">*</span></label>
                                                        <div class="cal-icon"><input class="form-control datetimepicker" type="text" id="editacademictxtenddate" name="editacademictxtenddate"></div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="m-t-20 text-center">
